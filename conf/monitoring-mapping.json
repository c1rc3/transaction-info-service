{
  "settings": {
    "number_of_shards": 5,
    "number_of_replicas": 0,
    "analysis": {
      "analyzer": {
        "default": {
          "tokenizer": "standard",
          "filter": [
            "asciifolding",
            "lowercase"
          ]
        }
      }
    }
  },
  "mapping": {
    "address": {
      "properties": {
        "id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "address": {
          "type": "string",
          "index": "not_analyzed"
        },
        "notify_type": {
          "type": "string",
          "index": "not_analyzed"
        },
        "receiver": {
          "type": "string",
          "index": "not_analyzed"
        },
        "created_time": {
          "type": "long"
        },
        "updated_time": {
          "type": "long"
        },
        "is_active": {
          "type": "boolean"
        }
      }
    }
  }
}